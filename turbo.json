{
    "$schema": "./node_modules/turbo/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "!.next/cache/**"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "type-check": {
            "dependsOn": ["^type-check"]
        }
    }
}
